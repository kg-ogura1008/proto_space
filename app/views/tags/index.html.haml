.jumbotron
  .container.text-center
    = image_tag("header_logo.png", alt: "PROTO TYPE - SHARE THE PROTOTYPE", height: "73px")
.filter-nav
  .container
    .row
      %ul.nav.nav-pills.col-lg-11
        %li.btn-default.btn-popular
          = link_to "Popular", "#"
        %li.btn-default.btn-popular
          = link_to "Newest", "#"
      = link_to "VIEW TAGS", "#", class: "btn btn-default col-lg-1"
.container.proto-list
  .row
    - @tags.each do |tag|
      .col-sm-4.col-md-3.proto-content
        .thumbnail
          %a{href: "#"}
            %button.btn.btn-tag{name: "button",type: "submit"}
              #{tag.name}
            %a.label.label-default.tag-list-number{href: "#"}
              #{tag.taggings_count}
              %i projects
